<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiCSharpMarkupExample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiCSharpMarkupExample">

    <ShellContent
        Title="Hello, World!"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Hello, World from C#!"
        ContentTemplate="{DataTemplate local:MainPageCSharp}"
        Route="MainPageCSharp" />

    <ShellContent
        Title="Hello, World from C# Markup!"
        ContentTemplate="{DataTemplate local:MainPageCSharpMarkup}"
        Route="MainPageCSharpMarkup" />
</Shell>
